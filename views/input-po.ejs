<%- include('header', { title: "input po"})%>
<h1 class="container justify-content-center form-container">
  Input PO
</h1>
<div class="container justify-content-center form-container">
  <div class="entry-single">
  <form action="/input" method="POST">

    <label for="client_po_no">Client PO No.:</label><br/>
    <input type="integer" id="client_po_no" name="quantity"><br/>
    
     <label for="product">Product:</label><br />
    <select name="product_id">
      <% productList.forEach(e =>{%>
      <option value="<%=e.id%>"><%=e.description%></option>
      <%}); %>
    </select>
    <br/>

    <label for="shipment_date">Shipment Date:</label><br/>
    <input type="date" id="shipment_date" name="shipment_date"/><br />

    <label for="quantity">Quantity</label><br/>
    <input type="integer" id="quantity" name="quantity">

    <input type="submit" value="Submit" />
     </form>
    

   
  </div>
</div>
<%- include('footer')%>



